public interface PrincipalRetas {

	public static void main(String[] argumentos) {
		
		Ponto2D pontoA = new Ponto2D(3.00,5.00);		
		Ponto2D pontoB = new Ponto2D(1.00,7.00);
		Ponto2D pontoC = new Ponto2D();
		
		RetasNoPlano reta1 = new RetasNoPlano(pontoA,pontoB);
		RetasNoPlano reta2 = new RetasNoPlano(pontoA);
		RetasNoPlano reta3 = new RetasNoPlano();
		RetasNoPlano reta4 = new RetasNoPlano(0.00,6.00,9.00,61.00);
		
		System.out.println("A equação da reta1 é: "+reta1);
		System.out.println("A equação da reta2 é: "+reta2);
		System.out.println("A equação da reta3 é: "+reta3);
		System.out.println("A equação da reta4 é: "+reta4);
		
		System.out.println("As retas 1 e 2 são "+reta1.comparacaoEntreRetas(reta2));
		System.out.println("As retas 3 e 3 são "+reta3.comparacaoEntreRetas(reta3));
		System.out.println("As retas 3 e 4 são "+reta3.comparacaoEntreRetas(reta4));
		
	}
	
	
	
}
